{
	"name": "netsuite-js",
	"description": "A Node wrapper for the NetSuite SOAP API.",
	"version": "0.0.5",
	"main": "lib/index.js",
	"author": {
		"name": "Christian Yang",
		"email": "christian.yang@crosslead.com",
		"url": "http://crosslead.com/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Navomi-Jaden/netsuite-js"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/Navomi-Jaden/netsuite-js",
	"bugs": "https://github.com/Navomi-Jaden/netsuite-js/issues",
	"config": {
		"blanket": {
			"pattern": "lib",
			"data-cover-never": [
				"node_modules",
				"test"
			]
		}
	},
	"scripts": {
		"lint": "eslint --config .eslintrc.json lib/. test/. --fix",
		"lint-nofix": "eslint --config .eslintrc.json lib/. test/.",
		"mocha": "NODE_ENV=test mocha test/ --recursive --require blanket --reporter mocha-lcov-reporter",
		"coverage": "YOURPACKAGE_COVERAGE=1 codecov -t 4988d6fc-f3a6-459c-95c1-e7c10125b98a",
		"test": "yarn lint && yarn mocha && yarn coverage"
	},
	"dependencies": {
		"denodeify": "^1.2.1",
		"lodash": "^4.17.4",
		"soap": "^0.23.0"
	},
	"devDependencies": {
		"async": "^2.6.0",
		"blanket": "^1.2.3",
		"codecov": "^3.0.0",
		"eslint": "^4.10.0",
		"mocha": "^4.0.1",
		"mocha-lcov-reporter": "^1.3.0",
		"nock": "^9.1.0",
		"should": "^13.1.3"
	}
}
